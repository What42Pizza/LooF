#include Basics.LOOF



function ConvertTableToString  -> TableIn, Seperator
	returnIf (typeOf TableIn ~= "table")
	default Seperator = ", "
	
	StringOut = toString TableIn[i]
	loop i, 1, lengthOf TableIn - 1
		StringOut = StringOut .. Sepeator .. (toString TableIn[i])
	repeat
	
	return {StringOut}
end